{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": ["src"],
		"includePattern": "\\.(js)$",
		"excludePattern": "(node_modules|lib|test)"
	},
	"plugins": ["jsdoc-plugin-typescript", "plugins/markdown", "./template/plugins/es6-fix"],
	"typescript": {
		"moduleRoot": "src"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"applicationName": "IdGenerator",
		"footer": "Made with ValeeraJS",
		"copyright": "ValeeraJS Copyright Â© 2020-2030 HypnosNova.",
		"disqus": "",
		"googleAnalytics": "UA-103772589-5",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "IdGenerator API Documentation",
			"description": "Documentation for IdGenerator library",
			"keyword": "docs, documentation, IdGenerator, javascript, jsdoc, valeerajs"
		},
		"linenums": true
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": false
	},
	"opts": {
		"encoding": "utf8",
		"recurse": true,
		"lenient": true,
		"destination": "./docs",
		"readme":"README.md",
		"template": "./node_modules/@pixi/jsdoc-template"
	}
}
